# Editor configuration, see https://editorconfig.org
root = true

[*]
charset = utf-8
indent_style = space
indent_size = 2
insert_final_newline = true
trim_trailing_whitespace = true

[*.{gradle,java,swift,xml}]
indent_size = 4

[*.plist]
indent_style = tab
indent_size = 4

[*.md]
indent_size = 4
max_line_length = off
trim_trailing_whitespace = false
