{
  "name": "ionic-angular-tailwindcss-starter",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "ng build",
    "fmt": "prettier --write . && ng lint --fix",
    "lint": "ng lint",
    "ng:update-angular-cli": "ng update @angular/cli --force && prettier --write package.json",
    "ng:update-angular-core": "ng update @angular/core --force && prettier --write package.json",
    "ng:update-angular-eslint": "ng update @angular-eslint/schematics --force && prettier --write package.json",
    "start": "ng serve"
  },
  "dependencies": {
    "@angular/common": "^17.3.9",
    "@angular/compiler": "^17.3.9",
    "@angular/core": "^17.3.9",
    "@angular/forms": "^17.3.9",
    "@angular/platform-browser": "^17.3.9",
    "@angular/platform-browser-dynamic": "^17.3.9",
    "@angular/router": "^17.3.9",
    "@ionic/angular": "^8.1.1",
    "ionicons": "^7.4.0",
    "rxjs": "^7.8.1",
    "tslib": "^2.6.2",
    "zone.js": "~0.14.5"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.3.7",
    "@angular-eslint/builder": "^17.4.1",
    "@angular-eslint/eslint-plugin": "^17.4.1",
    "@angular-eslint/eslint-plugin-template": "^17.4.1",
    "@angular-eslint/schematics": "^17.4.1",
    "@angular-eslint/template-parser": "^17.4.1",
    "@angular/cli": "^17.3.7",
    "@angular/compiler-cli": "^17.3.9",
    "@angular/language-service": "^17.3.9",
    "@ionic/angular-toolkit": "^11.0.1",
    "@types/node": "^20.12.11",
    "@typescript-eslint/eslint-plugin": "^7.10.0",
    "@typescript-eslint/parser": "^7.10.0",
    "eslint": "^8.57.0",
    "eslint-plugin-tailwindcss": "^3.15.1",
    "prettier": "^3.2.5",
    "prettier-plugin-organize-attributes": "^1.0.0",
    "prettier-plugin-organize-imports": "^3.2.4",
    "prettier-plugin-packagejson": "^2.5.0",
    "tailwindcss": "^3.4.3",
    "typescript": "~5.4.5"
  },
  "engines": {
    "node": ">=20.13.1",
    "pnpm": ">=9"
  }
}
